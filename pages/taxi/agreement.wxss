page {
  width: 100%;
  height: 100%;
}
.page {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  background: white;
}

.title {
  text-align: center;
  font-size: 32rpx;
  font-weight: bold;
  margin: 16rpx auto;
}

.p {
  line-height: 36rpx;
  font-size: 26rpx;
  color: #101010;
  margin-top: 20rpx;

}

.p-sub {
  line-height: 36rpx;
  font-size: 26rpx;
  color: #101010;
  margin-top: 5rpx;
  margin-left: 20rpx;
}

.blank {
  flex: 1 0 0%;
}

.agree {
  margin: 50rpx auto;
  width: 40%;
  height: 88rpx;
  background-color: #FF6091;
  color: white;
  font-size: 26rpx;
  text-align: center;
  line-height: 88rpx;
}